#ifndef SYMBOL
	#define SYMBOL(symbol, name)
#endif

// markers
SYMBOL (Article,  "@")
SYMBOL (Heading,  "=")
SYMBOL (Number,   "#")
SYMBOL (Bullet,   "*")

// separators
SYMBOL (Line,       "----")
SYMBOL (Header,     "|=")
SYMBOL (CodeBegin,  "{{{")
SYMBOL (CodeEnd,    "}}}")
SYMBOL (LineBreak,  "\\\\")
SYMBOL (Space,      "space")
SYMBOL (Tab,        "tabulator")
SYMBOL (Newline,    "new-line character")

// formats
SYMBOL (Italic,      "//")
SYMBOL (Bold,        "**")
SYMBOL (LinkBegin,   "[[")
SYMBOL (LinkEnd,     "]]")
SYMBOL (LabelBegin,  "<<")
SYMBOL (LabelEnd,    ">>")
SYMBOL (Pipe,        "|")

// literals
SYMBOL (String,  "string")
SYMBOL (Code,    "code")
SYMBOL (URL,     "URL")

#undef SYMBOL
