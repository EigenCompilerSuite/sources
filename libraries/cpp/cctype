#ifndef ECS_CPP_CCTYPE_HEADER_INCLUDED
#define ECS_CPP_CCTYPE_HEADER_INCLUDED

namespace std
{
	extern "C" auto isalnum (int) noexcept -> int;
	extern "C" auto isalpha (int) noexcept -> int;
	extern "C" auto isblank (int) noexcept -> int;
	extern "C" auto iscntrl (int) noexcept -> int;
	extern "C" auto isdigit (int) noexcept -> int;
	extern "C" auto isgraph (int) noexcept -> int;
	extern "C" auto islower (int) noexcept -> int;
	extern "C" auto isprint (int) noexcept -> int;
	extern "C" auto ispunct (int) noexcept -> int;
	extern "C" auto isspace (int) noexcept -> int;
	extern "C" auto isupper (int) noexcept -> int;
	extern "C" auto isxdigit (int) noexcept -> int;
	extern "C" auto tolower (int) noexcept -> int;
	extern "C" auto toupper (int) noexcept -> int;
}

#endif // ECS_CPP_CCTYPE_HEADER_INCLUDED
